{
  "name": "Janus",
  "description": "A server that integrates persona with CouchDB",
  "version": "0.0.6",
  "homepage": "https://github.com/pouchdb/janus",
  "author": {
    "name": "Dale Harvey",
    "email": "dale@arandomurl.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/daleharvey/couch-persona.git"
  },
  "main": "./lib/janus.js",
  "bin": {
    "janus": "./lib/janus.js"
  },
  "dependencies": {
    "http-proxy": "*",
    "underscore": "*",
    "async": "*",
    "express": "*",
    "cookie-parser": "*",
    "body-parser": "*",
    "request": "*",
    "node-uuid": "*",
    "winston": "*",
    "mkdirp": "*",
    "jshint": "~2.3.0"
  },
  "devDependencies": {
    "watchify": "~0.4.1",
    "browserify": "~3.24.13",
    "mocha": "~1.18.2",
    "supertest": "~0.11.0",
    "nodemon": "*"
  },
  "scripts": {
    "janus": "./lib/janus.js",
    "jshint": "jshint -c .jshintrc lib/*.js tests/*.js",
    "dev" : "./bin/dev.sh",
    "build-client": "browserify www/js/janus.js -s Janus > www/js/janus-client.js",
    "test": "npm run jshint && ./bin/run-test.sh"
